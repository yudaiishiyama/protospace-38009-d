<div class="card">
  <%= link_to image_tag prototype.image.variant(resize: '300x300'),prototype_path(prototype.id), method: :get ,if prototype.image.attached? %>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), class: :card__title%>
    <p class="card__summary">
      <%= prototype.concept %>
    </p>
    <%= link_to  prototype.user.name, user_path(prototype.user.id), method: :get, class: :card__user %>
  </div>
</div>